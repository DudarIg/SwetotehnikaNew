<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">
    <style name="Title">
        <item name="android:textSize">@dimen/title_size</item>
        <item name="android:textColor">@color/yelow</item>
    </style>
    <style name="BottomNavThem" parent="Theme.MaterialComponents.Light.NoActionBar">
        <item name="colorPrimary">@color/colorPrimaryRed</item>
    </style>
    <style name="chip_color" parent="Widget.MaterialComponents.Chip.Action">
        <item name="android:textColor">@color/black</item>
        <item name="chipIconTint">@color/colorPrimaryRed</item>
    </style>
    <style name="Theme.Swetotehnika.Splash">
        <item name="android:windowBackground">@drawable/splash_background_drawable</item>
    </style>

    <style name="MyActionBarTitle" parent="@style/TextAppearance.AppCompat.Widget.ActionBar.Title">
        <item name="android:textColor">@color/yelow</item>
    </style>
    <style name="MyBar" parent="@style/Widget.AppCompat.Light.ActionBar.Solid">
        <item name="background">@drawable/side_nav_bar</item>
        <item name="titleTextStyle">@style/MyActionBarTitle</item>
    </style>

</resources>